<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="344.0" prefWidth="428.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GymDataController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #2E4057;">
         <children>
            <Label text="MagiGym" textAlignment="CENTER" textFill="#fcfcfc">
               <font>
                  <Font name="Bernard MT Condensed" size="30.0" />
               </font>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <HBox.margin>
                  <Insets bottom="18.0" left="150.0" top="20.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox prefHeight="256.0" prefWidth="402.0" style="-fx-background-color: #2E4057;">
         <children>
            <Label prefHeight="19.0" prefWidth="102.0" text="SELECT USER:" textFill="WHITE">
               <HBox.margin>
                  <Insets bottom="30.0" left="20.0" right="50.0" top="50.0" />
               </HBox.margin>
               <font>
                  <Font name="SansSerif Regular" size="12.0" />
               </font>
            </Label>
            <ChoiceBox fx:id="selectUserChoiceBox" prefHeight="25.0" prefWidth="150.0">
               <HBox.margin>
                  <Insets bottom="30.0" right="100.0" top="50.0" />
               </HBox.margin>
            </ChoiceBox>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #2E4057;">
         <children>
            <Label prefHeight="16.0" prefWidth="104.0" text="PASSWORD:" textFill="WHITE">
               <font>
                  <Font name="SansSerif Regular" size="14.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="18.0" left="20.0" right="50.0" top="-100.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="passwordTextFiled">
               <HBox.margin>
                  <Insets bottom="18.0" right="100.0" top="-100.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#logIn" prefHeight="25.0" prefWidth="66.0" text="Log In" textFill="#354251">
         <VBox.margin>
            <Insets bottom="50.0" left="180.0" right="30.0" top="-100.0" />
         </VBox.margin>
      </Button>
      <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #2E4057;">
         <children>
            <Label text="Don't Have An Account?" textFill="WHITE">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <Button mnemonicParsing="false" onAction="#createAccount" text="Create Account" textFill="#2e4057">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
